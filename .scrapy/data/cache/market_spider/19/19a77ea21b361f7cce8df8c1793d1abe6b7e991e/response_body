<!-- 注册登录ssi 开始 -->
<!--sm2-->
<script type="text/javascript" src="/images/sm2.js"></script>

<!--login.js-->
<script>
    //长武新的login.js 20241015 code=SN000000737
    (function(window,localStorage,undefined){
        var LS = {
                set : function(key, value){
                    //在iPhone/iPad上有时设置setItem()时会出现诡异的QUOTA_EXCEEDED_ERR错误
                    //这时一般在setItem之前，先removeItem()就ok了
                    if( this.get(key) !== null )
                        this.remove(key);
                    localStorage.setItem(key, value);
                },
                //查询不存在的key时，有的浏览器返回undefined，这里统一返回null
                get : function(key){
                    var v = localStorage.getItem(key);
                    return v === undefined ? null : v;
                },
                remove : function(key){ localStorage.removeItem(key); },
                clear : function(){ localStorage.clear(); },
                each : function(fn){
                    var n = localStorage.length, i = 0, fn = fn || function(){}, key;
                    for(; i<n; i++){
                        key = localStorage.key(i);
                        if( fn.call(this, key, this.get(key)) === false )
                            break;
                        //如果内容被删除，则总长度和索引都同步减少
                        if( localStorage.length < n ){
                            n --;
                            i --;
                        }
                    }
                }
            },
            j = window.jQuery, c = window.Core;
        //扩展到相应的对象上
        window.LS = window.LS || LS;
        //扩展到其他主要对象上
        if(j) j.LS = j.LS || LS;
        if(c) c.LS = c.LS || LS;
    })(window,window.localStorage);

    var searchUrl=window.location.href;
    var SSOSignOrder= /code=(\w*)/;
    var SSOSign = searchUrl.match(SSOSignOrder);
    SSOSign = SSOSign ? SSOSign [1] : '';
    if(SSOSign=="" || SSOSign==null||SSOSign==undefined) {
        SSOSign = localStorage.SSOSign;
    }
    console.log("cw SSOSign-->",SSOSign)

    function userInfoByKey(){
        $.ajax({
            type: "get",//方法类型
            dataType: "json",//预期服务器返回的数据类型
            url: "/fsp/login/userInfoByKey" ,//url
            contentType:"application/json",
            headers: {
                SSOSign: SSOSign
            },
            success: function (result) {
                if(result.code==200){
                    var secretKey = result.summary_info.secretKey;
                    var userdata = result.datas;
                    $.ajax({
                        type: "get",//方法类型
                        dataType: "json",//预期服务器返回的数据类型
                        url: "/fsp/login/getPrivateKey?secureKey="+secretKey ,//url
                        contentType:"application/json",
                        headers: {
                            SSOSign: SSOSign
                        },
                        success: function (result) {

                            var decryptData = sm2.doDecrypt(userdata,result.datas.secretvalue, 1)
                            decryptData = JSON.parse(decryptData);
                            console.log("decryptData->",decryptData);
                            if(result.code==200){
                                localStorage.SSOSign=SSOSign;
                                $("#dlxx").text("你好，"+decryptData.nickname);

                                $("#dlxx").attr("href","https://zwfw.shaanxi.gov.cn/icity/uinfo");
                                $("#zcxx").text("退出");
                                $("#zcxx").attr("href","javascript:logout();");
                                //fix start
                                $("#zcxx").removeAttr('href');
                                $("#zcxx").on("click",function(){
                                    logout();
                                });
                                //fix end

                                if(decryptData.acctType == "10"){
                                    $("#dlxx").text("你好，"+decryptData.username);                                   
                                    //$("#dlxx2").text(decryptData.username);
                                }else{
                                    $("#dlxx").text("你好，"+decryptData.corpname);                                    
                                    //$("#dlxx2").text(decryptData.corpName);
                                }
                                //$("#dlxx2_a").attr("href","javascript:void(0)");
                                //$("#zcxx2").text("退出");
                                //$("#zcxx2_a").attr("href","javascript:logout();");
                                //addTrack();
                            }else{
                                //$("#modalAuth").css('display','block');
                                //$("#mask").css('display','block');
                                //$("#loginuser").attr("href",(result.link[0].url+window.location.href));
                            }
                        },
                        error : function(result) {
                        }
                    });
                }else{
                    $("#zcxx").attr("href",(result.link[0].url+window.location.href.replace(/\?.*$/, "")).replace("amp;",""));
                    $("#dlxx").attr("href",(result.link[0].url+window.location.href.replace(/\?.*$/, "")).replace("amp;",""));
                    
                }
            },
            error : function(result) {
            }
        });
    }
    userInfoByKey();

    function logout() {
        $.ajax({
            type: "get", //方法类型
            dataType: "json", //预期服务器返回的数据类型
            url: "/fsp/login/loginOut", //url
            contentType: "application/json",
            beforeSend: function (XMLHttpRequest) {
                XMLHttpRequest.setRequestHeader("SSOSign", SSOSign);
            },
            success: function (result) {
                if (result.code == 200) {
                    localStorage.removeItem("SSOSign");
                    // window.location.href = (result.datas + window.location.href);
                    window.location.href = window.location.href.substr(0, window.location.href.indexOf("?"));
                    alert(result.msg);
                }
            },
            error: function (result) {
                alert("退出失败，请稍后再试");
            }
        });
    }

</script>
<!-- 注册登录ssi 结束 -->

