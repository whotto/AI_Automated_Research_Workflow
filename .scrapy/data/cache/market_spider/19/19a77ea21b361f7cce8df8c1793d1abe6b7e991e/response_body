        <script>
          var uri = window.location.pathname;
          $("#ty-nav li").each(function () {
            $(this).removeClass("hover");
          });
          if (uri.indexOf("/xw/") >=0) {
            $("#nav2").addClass('hover');

          } else if (uri.indexOf("/zfxxgk/") >=0) {
            $("#nav3").addClass('hover');

          } else if (uri.indexOf("/fw/") >=0) {
            $("#nav4").addClass('hover');

          } else if (uri.indexOf("/hd/") >=0) {
            $("#nav5").addClass('hover');

          } else if (uri.indexOf("/xq/") >=0) {
            $("#nav6").addClass('hover');

          } else {
            $("#nav1").addClass('hover');
          }
        </script>
      </div>
