<script type="text/javascript">
    $(function () {
        var shareTitle = encodeURIComponent("2016年7月27日长武县环保局受理建设项目环境影响评价文件公示");
        var curUrl = encodeURIComponent(window.location.href);
        var picUrl = encodeURIComponent("");
        var shareUrl = "http://service.weibo.com/share/share.php?url=" + curUrl + "&title=" + shareTitle + "&pic=" + picUrl;
