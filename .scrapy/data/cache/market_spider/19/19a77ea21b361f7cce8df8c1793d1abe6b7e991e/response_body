<script>
  // 搜索
  function submitsearch() {
    var result = $("#q").val();
    result = $.trim(result);
    var reg = /[`~!@#$%^&*_<>{}\/'[\]]/im;

    var url = window.location.href;
    var arrUrl = url.split("//");
    var arrUrl1 = arrUrl[1].split("/");
    var para = arrUrl1[0];
    console.log(para);

    if (reg.test(result)) {
      alert("您输入的信息包含非法字符!");
      return false;
    }
    if (result == "请输入关键字搜索" || result == "" || result == null) {
      alert("请输入关键字搜索");
      return false;
    } else {
      result = result.replace("-", "\-");
      result = result.replace("(", "\(");
      result = result.replace(")", "\)");
    }
    window.open("http://" + para + "/so/s?qt=" + encodeURI(result));
    //window.open("http://" + para + "/irs-c-web/search.html?code=17cb6217c62&searchWord=" + encodeURI(result) + "&tenantid=82");
    //window.open("http://10.63.32.142/irs-c-web/search.html?code=17cb6217c62&searchWord=" + encodeURI(result) + "&tenantid=82");
  }
</script>
<script type="text/javascript" src="/images/esdToolsInit.js"></script>
