<!--dropdown-toggle-->	
<script>
$('.dropdown-toggle').on('click', function(e){
	  e.preventDefault();
	  $(this).siblings('.dropdown-box').toggleClass('active');
	  $(this).toggleClass('open');
	})
</script>
	
