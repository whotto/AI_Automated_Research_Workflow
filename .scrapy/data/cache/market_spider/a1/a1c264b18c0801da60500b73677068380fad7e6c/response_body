<script>
$(function(){
	var navArr = ["news","zhengce","lianxhy","shidian","lingyu","difang","tongji","yanjiu","zhishi"];
	var path = window.location.pathname;
	var arr = (path ? path : '').split("/", 3);
	if(arr.length==3){
		$.each(navArr,function(i,obj){
			if(arr[2]==obj){
				$("li.level-1:eq(0)").removeClass("cur");
				$("li.level-1:eq("+(i+1)+")").addClass("cur");
				return false;
			}
		});
	}
});

