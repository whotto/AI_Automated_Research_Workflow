<script type="text/javascript">
$(function(){
	var c = $(".article-con-01").text().indexOf("an error occurred while processing this directive");
	if(c>0){
	window.location.href="/index.shtml";
	//$(".article-con-01").html("该文章已删除，请浏览其他信息");
	}else{
	document.title=title; 
	$("#show_title").html(title);
	$("#show_time").html(publishTime.substring(0,10) + " 文章来源：" + source);
	if(digest == null || digest == ""){
		$("#digest").hide();
	}else {
		$("#digest").html(digest);
	}
	
	
	var path = window.location.pathname;
	var channel=path.substring(9, path.indexOf("/", 9));
	if(s_id!=null && s_id!=''){
		$.ajax({
			url:"/TradeGuide_CMS/tradeFront/getAccessary",
			data:{"sid":s_id},
			dataType:"json",
			type:"post",
			success:function(data){
				var html='';
				$.each(data,function(i,obj){
					html += '<a href="'+obj.ACURL+'">';
					html += obj.ACNAME;
					html +='</a>';
				});
				$("#accessaryspan").html(html);
			}
		});	
	}
	$.ajax({
		url:"/TradeGuide_CMS/tradeFront/getRelatedArticles",
		data:{"keyword":keyword,"articleId":articleId,"channel":channel},
		dataType:"json",
		type:"post",
		success:function(data){
			if(data!=null){
				$("#ralatetext").addClass("am-tit-01").html("<h5>相关文章</h5>");
				$("#related_article").addClass("textList_03");
			}
			var html = '';
			$.each(data,function(i,obj){
				html += '<li><a target="_blank" href="/article'+obj.fullNameEN +obj.htmlUrl+'">';
				html += obj.title;
				html += '</a></li>'
			});
			$("#related_article").html(html);
		}
	});	
	$.ajax({
		url:"/TradeGuide_CMS/front/getClickNum",
		data:{"fdid":articleId},
		dataType:"html",
		type:"post",
		success:function(result){
		//	$("#clickNum").html("  浏览次数：" + result);
		}
	});
	
}	
});
</script>
